{
  "name": "teltonika-data-receiver",
  "version": "1.2.0",
  "description": "Receive GPS data from a Teltonika (RUT955) device",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run lint && tsc --version && tsc --pretty",
    "clean": "rimraf dist",
    "lint": "tslint --force --format verbose \"src/**/*.ts\" \"test/**/*.ts\" \"examples/**/*.ts\"",
    "test": "mocha --require ts-node/register --recursive **/*_spec.ts",
    "prepare": "npm run test && npm run clean && npm run lint && tsc --version && tsc --pretty --declaration"
  },
  "author": "Thomas Stig Jacobsen <tsj@lunarway.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/loglevel": "1.5.3",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.18",
    "chai": "4.2.0",
    "mocha": "5.2.0",
    "rimraf": "2.6.2",
    "ts-node": "7.0.1",
    "tslint": "5.12.0",
    "tslint-config-standard": "8.0.1",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.2.2"
  },
  "dependencies": {
    "decimal.js": "10.0.2",
    "loglevel": "1.6.1"
  }
}
